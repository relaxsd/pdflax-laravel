{
  "name": "relaxsd/pdflax-laravel",
  "description": "Laravel 5 wrapper for Pdflax.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martijn Kriek",
      "email": "martijn.kriek@relax-software.nl"
    }
  ],
  "require": {
    "illuminate/support": "~5.5",
    "php": ">=7.0",
    "relaxsd/pdflax": "^0.8.0",
    "relaxsd/pdflax-fpdf": "^0.8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Relaxsd\\Pdflax\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Relaxsd\\Pdflax\\Laravel\\PdflaxServiceProvider"
      ],
      "aliases": {
        "Pdflax": "Relaxsd\\Pdflax\\Laravel\\PdflaxFacade"
      }
    }
  }
}
